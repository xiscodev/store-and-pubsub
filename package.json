{
  "name": "store-and-pubsub",
  "version": "1.0.0",
  "description": "Library to store values in user defined paths, also can be subscribed to store published changes through pub sub flow",
  "author": "Jos√© Diaz <contact@xisco.dev> (https://xisco.dev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/xoxefdp/store-and-pubsub.git"
  },
  "keywords": [
    "javascript",
    "browser",
    "store",
    "pubsub"
  ],
  "engines": {
    "node": ">= 6.8.0"
  },
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack --progress --config webpack.config.js",
    "build:dev": "NODE_ENV=development npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "clean": "rimraf dist",
    "postclean": "npm run test:cover",
    "predocs": "npm run test:cover",
    "docs": "documentation readme src/** --section=JSDOC",
    "docs:watch": "npm run docs -- --watch",
    "test": "npm run lint && jest --verbose --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:watch-all": "npm run test -- --watchAll",
    "test:cover": "npm run test -- --coverage",
    "lint": "eslint --fix \"src/*.js\""
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "documentation": "^13.2.5",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "^34.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "jest-runner-eslint": "^0.10.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2",
    "pubsub-js": "^1.9.3",
    "the-type-validator": "^1.1.1"
  }
}
